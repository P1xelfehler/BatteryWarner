    <string name="intro_finish_toast">Let\'s make your battery last longer!</string>
    <string name="intro_slide_1_description">It will help your battery to live longer!</string>
    <string name="intro_slide_1_title">Welcome to Battery Warner!</string>
    <string name="intro_slide_2_description" formatted="false">Manufacturers say that your battery has a longer life, if you do not let the battery too long at 100% or reach 0%!</string>
    <string name="intro_slide_2_title">How does it do that?</string>
    <string name="intro_slide_3_description" formatted="false">That means keeping the battery level for example between 20–80% instead of 0–100%!</string>
    <string name="intro_slide_3_title">For the longest life, you have to charge \"flat\"!</string>
    <string name="intro_slide_prefs_description">Choose your settings right here!</string>
    <string name="intro_slide_prefs_title">Let\'s get started!</string>


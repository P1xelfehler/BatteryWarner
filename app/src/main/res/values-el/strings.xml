    <string name="intro_advanced_mode">Advanced mode</string>
    <string name="intro_advanced_mode_description">The advanced mode makes root settings and more available - everything the app has to offer.</string>
    <string name="intro_easy_mode">Easy mode</string>
    <string name="intro_easy_mode_description">The easy mode is recommended for most users. Only the most important settings are available.</string>
